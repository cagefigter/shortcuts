If Equals "getUserOpts"
  GetFile a{
    service="iCloud Drive"
    showDocumentPicker=false
    filePath="\(v:opt:"storage.dir")/\(v:opt:"storage.file")"
    errorIfnotFound=false
  } -> v:usropts
  Count Items
  If Equals 0
    Dictionary {
        <bool> ShowAlerts : true,
    } -> v:usropts
  End
  GetVariable v:usropts
  ExitShortcut 
End